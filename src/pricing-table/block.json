{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fbs-block/pricing-table",
	"version": "0.1.0",
	"title": "Pricing Table",
	"category": "fbs-blocks",
	"icon": "smiley",
	"description": "only for testing",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Our Pricing"
		},
		"starterTitle": {
			"type": "string",
			"default": "Starter"
		},
		"starterPrice": {
			"type": "number",
			"default": "19.99"
		},
		"premiumTitle": {
			"type": "string",
			"default": "Premium"
		},
		"premiumPrice": {
			"type": "number",
			"default": "49.99"
		},
		"businessTitle": {
			"type": "string",
			"default": "Business"
		},
		"businessPrice": {
			"type": "number",
			"default": "99.99"
		}
	},
	"textdomain": "fbs-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
