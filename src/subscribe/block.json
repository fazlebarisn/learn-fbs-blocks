{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fbs-block/subscribe",
	"version": "0.1.0",
	"title": "Subscribe",
	"category": "fbs-blocks",
	"icon": "smiley",
	"description": "Add a subscribe button",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"source": "text",
			"selector": "h2",
			"default": "Let's keep in touch"
		},
		"description": {
			"type": "string",
			"source": "text",
			"selector": "p",
			"default": "Subscribe to keep up with fresh news and exciting updates. We promise not to spam you!"
		},
		"placeholder": {
			"type": "string",
			"source": "text",
			"default": "Enter your email address"
		},
		"button": {
			"type": "string",
			"source": "text",
			"default": "Send"
		}
	},
	"textdomain": "fbs-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
